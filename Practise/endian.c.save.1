#include<stdio.h>
int main()
{
	int number = 0x41424345;
	char* char_pointer = (char*)(&number);
	int i = 0;
	for(i=0;i<4;i++)
	{
		printf("%s\n",char_pointer);
	}
	if(char_pointer[0] == 41)
	{
		printf("The compiler is Big Endian\n");
	}
	else
	{
		printf("The compiler is Little Endian");
	}
	return 0;
}
